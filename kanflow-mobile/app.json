{
  "expo": {
    "name": "kanflow-mobile",
    "slug": "kanflow-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090b"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uthybuilds.kanflow"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#09090b"
      },
      "package": "com.uthybuilds.kanflow"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "scheme": "kanflow",
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c45a6c5a-5e7f-49d3-8ef1-81d29dd3ad04"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c45a6c5a-5e7f-49d3-8ef1-81d29dd3ad04"
    }
  }
}
